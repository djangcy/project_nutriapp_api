{
  "compilerOptions": {
    "target": "es2017",
    "module": "node16",
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "moduleResolution": "node16",
    "baseUrl": "./src",
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "node_modules",
  ]
}
